{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "babel": "node_modules/.bin/babel src -d functions/ && node_modules/.bin/babel src/index.js -d functions",
    "test": "jest --coverage",
    "flow": "flow focus-check ./src/",
    "lint": "eslint",
    "eslint": "eslint --fix",
    "prettier": "prettier",
    "fmt": "prettier --config .prettierrc.yml --write \"{!(node_modules),!(__mock__)/**/}*.{jsx,js,json,md,ts}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "flow focus-check ./src/",
      "eslint --fix",
      "git add"
    ],
    "*.{jsx,js,json,md,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.93.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "webpack": "^4.29.3"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/polyfill": "^7.2.5",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "firebase": "^5.8.4",
    "firebase-admin": "~7.0.0",
    "firebase-functions": "^2.2.0"
  }
}
